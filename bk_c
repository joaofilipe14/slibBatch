-- Name: time_slot; Type: TABLE; Schema: rms; Owner: rmt_iat
--

CREATE TABLE rms.time_slot (
    id numeric(19,0) NOT NULL,
    access_code character varying(10) NOT NULL,
    id_origin numeric(19,0),
    slot time without time zone NOT NULL,
    risk_unit_id numeric(19,0) NOT NULL,
    version numeric(10,0),
    unique_id character varying(64)
);


ALTER TABLE rms.time_slot OWNER TO rmt_iat;

