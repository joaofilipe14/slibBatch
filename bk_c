--
-- TOC entry 481 (class 1259 OID 31795)
-- Name: var_parameters; Type: TABLE; Schema: rms; Owner: rmt_iat
--

CREATE TABLE rms.var_parameters (
    id numeric(19,0) NOT NULL,
    access_code character varying(10) NOT NULL,
    alpha_all numeric(19,4),
    alpha_all_buy numeric(19,4),
    alpha_all_sell numeric(19,4),
    alpha_all_trx numeric(19,4),
    alpha_certain numeric(19,4),
    closing_percentile_point numeric(19,0),
    exclude_uncertain boolean DEFAULT false NOT NULL,
    factor_exposure_1 numeric(19,4),
    factor_exposure_2 numeric(19,4),
    first_percentile_point numeric(19,0),
    haircut numeric(19,4),
    id_origin numeric(19,0),
    risk_adj_pc numeric(19,4),
    risk_adj_sd numeric(19,4),
    settle_ratio_buy numeric(3,2) DEFAULT 0 NOT NULL,
    settle_ratio_sell numeric(3,2) DEFAULT 0 NOT NULL,
    timehorizon numeric(10,0),
    var_engine character varying(3),
    version numeric(10,0),
    addon_holding numeric(3,2) DEFAULT 0 NOT NULL,
    addon_total numeric(3,2) DEFAULT 0 NOT NULL,
    alpha_cmma numeric(19,4),
    unique_id character varying(64)
);


ALTER TABLE rms.var_parameters OWNER TO rmt_iat;

