--
-- TOC entry 479 (class 1259 OID 31790)
-- Name: time_slot_trigger; Type: TABLE; Schema: rms; Owner: rmt_iat
--

CREATE TABLE rms.time_slot_trigger (
    id numeric(19,0) NOT NULL,
    access_code character varying(10),
    id_origin numeric(19,0),
    slot time without time zone,
    day_of_week numeric(10,0),
    active boolean DEFAULT false NOT NULL,
    risk_unit_id numeric(19,0) NOT NULL,
    unique_id character varying(64) NOT NULL,
    version numeric(10,0)
);


ALTER TABLE rms.time_slot_trigger OWNER TO rmt_iat;

