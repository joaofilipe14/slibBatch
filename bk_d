    last_var_computation_date timestamp(6) without time zone,
    name character varying(32) NOT NULL,
    id_origin numeric(19,0),
    setup character varying(32) NOT NULL,
    var_computation_period numeric(10,0),
    version numeric(10,0),
    client_entity_id numeric(19,0) NOT NULL,
    mr_currency_ref_id character varying(3),
    reference_currency_ref_id character varying(3) NOT NULL,
    last_exposition_vacation_id numeric(19,0),
    var_parameters_id numeric(19,0) NOT NULL,
    branch character varying(3),
    crds_id character varying(12),
    credit_line_currency_ref_id character varying(3),
    credit_line_last_update timestamp(6) without time zone,
    lowest_final_exposure_cl numeric(30,9),
    lowest_fe_vacation_id numeric(19,0),
    btf boolean DEFAULT false,
    funding_credit_line numeric(30,9) DEFAULT '-1'::integer,
    funding_cl_last_update timestamp(6) without time zone,
    btf_haircut numeric(30,9),
    btf_cash_request_matching boolean DEFAULT false NOT NULL,
    description character varying(100),
    alert_group_id numeric(19,0),
    otc_min_settlement_date numeric(5,0),
    otc_max_settlement_date numeric(5,0),
    computation_mode_code character varying(32) DEFAULT 'FREQUENCY'::character varying NOT NULL,
    computation_min_bound time without time zone,
    computation_max_bound time without time zone,
    flux2_send_date timestamp(6) without time zone,
    btf_threshold numeric(21,0) DEFAULT 0 NOT NULL,
    critical_alert_level numeric(1,0),
    alert_level integer,
    unique_id character varying(64),
    bkt_start_time time without time zone,
    bkt_end_time time without time zone,
    bkt_timezone character varying(64),
    bkt_last_selection date,
    gcm boolean DEFAULT false NOT NULL,
    simulation boolean NOT NULL,
    ptf_sim_exec_id numeric,
    margining_enabled boolean NOT NULL,
    margining_config_id numeric(19,0),
    margining_formula_type character varying(32),
    minimum_deposit numeric(30,9),
    insert_date date,
    insert_date_time timestamp(6) without time zone,
    dual_mode boolean,
    bic_code character varying(12),
    df boolean DEFAULT false NOT NULL
);


